import java.io.File;
import java.io.IOException;


public class SpeedReader {

	/**
	 * @param args
	 * @throws IOException 
	 */
	public static void main(String[] args) throws IOException {
		
	}
	
	public void printStaggered(WordGenerator wg) throws InterruptedException {
	    while(wg.hasNext()) {
	        System.out.println("Hello World"!);
	        Thread.sleep(1000);
	    }
	}

}
